                  << END;
              return def;
            }()
         << []{
              auto exe = Stream{"Executor"sv};
              exe << add<Executor<library::Executor<NUMERIC>, library::Definition<NUMERIC>, library::Configuration<NUMERIC>>>()
                  << END;
              return exe;
            }()
