#divert <fpp>
#import tunitas.keyston.numeric.required.Element
#import tunitas.keyston.numeric.generator.required.Triple
namespace tunitas::keyston::numeric::generator::required {
  //
  // The constraints on the arguments to numeric::generator::Generator<...>
  //
  template<auto BASE, auto EXPONENT, auto MODULUS, typename SUBELEMENT, typename ELEMENT> concept Generator = requires(SUBELEMENT, ELEMENT) {
    requires Triple<BASE, EXPONENT, MODULUS, SUBELEMENT>;
    requires numeric::required::Element<ELEMENT>;
  };
}
#endiv
