#divert <fpp>
#import tunitas.keyston.numeric.required.Subelement
#import tunitas.integer.required.Large // ...... appears as integer::Large
#import tunitas.integer.required.Unsigned // ... appears as integer::Unsigned
namespace tunitas::keyston::numeric::required {
  //
  // An element in the numeric "field" is an (unsigned) "integer".
  //
  template<typename INTEGER> concept Element = (integer::Large<INTEGER> && integer::Unsigned<INTEGER>) || Subelement<INTEGER>;
}
#endiv
#divert <cpp>
#import tunitas.keyston.integer
namespace {
  namespace testate {
    using namespace tunitas::keyston::integer;
    namespace required = tunitas::keyston::numeric::required;
    //
    static_assert(required::Element<uint8_t>);
    static_assert(required::Element<uint16_t>);
    static_assert(required::Element<uint32_t>);
    static_assert(required::Element<uint64_t>);
    static_assert(required::Element<uint128_t>);
    static_assert(required::Element<uint256_t>);
    static_assert(required::Element<uint512_t>);
  }
}
#endiv
