#divert <fpp>
#forward tunitas.keyston.field.Element
#import tunitas.keyston.field.traits.Element
#include <tunitas.keyston.field.traits.Element>
namespace tunitas::keyston::field::traits {
  //
  // Re-identify the underlying type of the element
  //   enums are derived from integers .......... via the enum class definition.
  //   classes are derived from other classes ... via the class ancestor.
  // specializations only.
  //
#if 1
  template<typename TYPE> using Underlying = typename Element<TYPE>::Underlying;
#else // [[REMOVETHIS]]
  template<typename TYPE> struct [[deprecated("instead use traits::Element<TYPE>::Underlying")]] Underlying { using Type = typename Element<TYPE>::Underlying; };
#endif
}
#endiv
