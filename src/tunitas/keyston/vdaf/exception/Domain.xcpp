#divert <fpp>
namespace tunitas::keyston::vdaf::exception {
  //
  // Some value is out of the allowed domain in Tunitas Keyston VDAF.
  // The domain of a (mathematical) function is the input to such a function.
  //
  // Specification:
  //
  //   Yes.
  //
  //   Reminder: the exception for use with such a value when indexign is Bound
  //   Bound (index is out of range, std::out_of_range)
  //   Domain (value is out of range, std::range_error)
  //
  // Design:
  //
  //   More than a type alias.  There is no design.
  //
  // Usage:
  //
  //   Throw it!
  //
  struct [[nodiscard]] Domain;
}
#endiv
#divert <hpp>
#import tunitas.exception.complete.Basic
#import tunitas.exception.partial.Domain
#import tunitas.exception.Aspect
#import tunitas.keyston.exception.Aspect
#import tunitas.keyston.vdaf.exception.Aspect
class tunitas::keyston::vdaf::exception::Domain final : public complete::Basic<partial::Domain, vdaf::exception::Aspect, keyston::exception::Aspect, tunitas::exception::Aspect> {
  using Ancestor = complete::Basic<partial::Domain, vdaf::exception::Aspect, keyston::exception::Aspect, tunitas::exception::Aspect>;
public:
  using Ancestor::Basic;
};
#endiv
#divert <cpp>
#import std.derived_from
namespace {
  namespace testate {
    using tunitas::keyston::vdaf::exception::Domain;
    static_assert(std::derived_from<Domain, std::domain_error>);
  }
}
#endiv
