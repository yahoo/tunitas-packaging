#divert <hpp>
#import tunitas.keyston.protocol.id // for id::Job
#import tunitas.denniston.concurrent.dictionary.Dictionary
#import tunitas.denniston.job.usage.dictionary.Valuable
namespace tunitas::denniston::job {
  template<required::dictionary::Valuable VALUE> struct dictionary::Dictionary : public concurrent::dictionary::Dictionary<LOGARITHMIC, SEQUENTIAL, Parameters<keyston::protocol::id::Job, VALUE>> {
    using Ancestor = denniston::concurrent::dictionary::Dictionary<LOGARITHMIC, SEQUENTIAL, Parameters<keyston::protocol::id::Job, VALUE>>;
    static_assert(usage::dictionary::Valuable<VALUE>);
  public:
    Dictionary() = default;
    using Ancestor::Dictionary;
  };
}
