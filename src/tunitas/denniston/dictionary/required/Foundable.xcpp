#divert <fpp>
#import tunitas.denniston.dictionary.required.Either
namespace tunitas::denniston::dictionary::required {
  //
  // For the dictionary, is this type Foundable?
  //
  // Specification:
  //
  //   The constie or mutable variant
  //   MUST be diagnosable without seeing the complete type of struct dictionary::Dictionary
  //
  // Design:
  //
  //   Obvious.
  //
  //   Indeed, "foundable" is not grammatical; yet it conjugates with its use in diagnosing admissability for Found<VALUE>
  //
  // Usage:
  //
  //   See dictionary::Dictionary
  //
  template<typename FOUNDABLE, typename VALUE> concept Foundable = Either<FOUNDABLE, VALUE>;
}
