#divert <fpp>
#import tunitas.denniston.service.gateway.subprocess.Prototype
#include <hpp/tunitas.denniston.service.gateway.subprocess.Prototype>
static_assert(sizeof (tunitas::denniston::service::gateway::subprocess::Prototype));
#import tunitas.denniston.process.companion.Series
namespace tunitas::denniston::service::gateway {
  //
  // A collection of subprocesses
  //
  // Specification:
  //
  //   Ordered by creation-order.
  //   Does NOT lock.
  //   Provides destruction at end-of-life.
  //
  // Design:
  //
  //   Whereas a Subprocess (subprocess:::Prototype descendant) is immobile, it needs to be held as such.
  //
  // Usage:
  //
  //   See Gateway, which is nearby.
  //
  static_assert(std::derived_from<subprocess::Prototype, process::companion::Prototype<Gateway>>);
  using Subprocesses = process::companion::Series<subprocess::Prototype>;
}
#endiv
