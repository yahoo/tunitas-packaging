#forward tunitas.denniston.service.processor.subprocess.Continuation
namespace {
  namespace testate {
    namespace required = tunitas::denniston::service::processor::subprocess::required;
    using namespace tunitas::denniston::service::processor::subprocess;
    static_assert(required::Subprocess<Prototype>);
    static_assert(required::Subprocess<Boot>);
    static_assert(required::Subprocess<Initialization>);
    static_assert(required::Subprocess<Continuation>);
  }
}
