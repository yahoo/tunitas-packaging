#divert <fpp>
#import tunitas.denniston.service.processor.subprocess.traits.Subprocess
#include <hpp/tunitas.denniston.service.processor.subprocess.traits.Subprocess>
namespace tunitas::denniston::service::processor::subprocess::required {
  //
  // Is this TYPE a processor subprocess?
  // Must be answerable without the complete type of TYPE (i.e. no use of derived_from<...>)
  // Instead we require the complete type of the traits::Subprocess<SUBPROCESS> declaration.
  //
  template<typename SUBPROCESS> concept Subprocess = traits::Subprocess<SUBPROCESS>::value;
}
#endiv
#divert <cpp>
#forward tunitas.denniston.service.processor.subprocess.Prototype
#forward tunitas.denniston.service.processor.subprocess.Boot
