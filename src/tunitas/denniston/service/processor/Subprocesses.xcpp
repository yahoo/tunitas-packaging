#divert <fpp>
#import tunitas.denniston.service.processor.Subprocess
#import std.deque
namespace tunitas::denniston::service::processor {
  //
  // A collection of subprocesses
  //
  // Specification:
  //
  //   Ordered by creation-order.
  //   Does NOT lock.
  //   Provides destruction at end-of-life.
  //
  // Design:
  //
  //   Whereas a Subprocess (subprocess:::Prototype descendant) is immobile, it needs to be held as such.
  //
  // Usage:
  //
  //   See Processor, which is nearby.
  //
  using Subprocesses = std::deque<Subprocess>;
}
#endiv
