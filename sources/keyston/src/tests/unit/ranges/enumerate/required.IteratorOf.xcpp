#divert <fpp>
#forward tunitas.ranges.package_enumerate.Iterator
namespace tests::unit::ranges::enumerate::required {
  //
  // because nonstd::required::InstanceOf can't handle NTTP (Non-Type Template Parameters)
  //
  template<typename ITERATOR> concept IteratorOf = requires(ITERATOR) {
    { []<typename SUBITERATOR, auto MUTABILITY>(library_body::Iterator<SUBITERATOR, MUTABILITY> *)
      requires same_as<library_body::Mutability, decltype(MUTABILITY)>
      {}(static_cast<ITERATOR *>(nullptr)) };
  };
}
