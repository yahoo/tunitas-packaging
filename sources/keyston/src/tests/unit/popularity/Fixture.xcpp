#divert <hpp>
#import tunitas.keyston.exception.Aspect
struct tests::unit::popularity::Fixture : public rigging::suite::Fixture {
  static auto suite() -> Suite;
  //
  auto test_usage_Popularity() -> void;
  auto test_usage_Prefixes() -> void;
