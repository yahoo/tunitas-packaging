#divert <hpp>
#import tunitas.error.by.required.Indicator
#import tunitas.keyston.vdaf.Name
#import tests.unit.name.Name
struct tests::unit::vdaf::eponymous_name::Name : public tests::unit::name::Name<library::Name, tests::unit::vdaf::Name> {
  using Subject = library::Name;
  template<tests::unit::name::required::Indicator> auto test_to_string_good() -> void;
  template<::tunitas::error::by::required::Indicator> auto test_to_string_fail() -> void;
  template<required::Indicator> auto test_to_string_view_good() -> void;
  template<required::Indicator> auto test_to_string_view_fail() -> void;
};
#endiv
#divert <tpp>
#import tests.unit.name.To_String
#import tests.unit.name.To_String_View
namespace tests::unit::vdaf::eponymous_name {
  template<required::Indicator INDICATOR> auto Name::test_to_string_good() -> void {
    To_String<INDICATOR, Subject>::exercise_good(Subject::ZERO, Subject::CONSTANT, Subject::PRIO3_COUNT, Subject::PRIO3_SUM, Subject::PRIO3_HISTOGRAM, Subject::POPLAR1);
  }
  template<required::Indicator INDICATOR> auto Name::test_to_string_fail() -> void {
    To_String<INDICATOR, Subject>::exercise_fail(Subject{999'999'999u}, Subject{888'888'888});
  }
  template<required::Indicator INDICATOR> auto Name::test_to_string_view_good() -> void {
    To_String_View<INDICATOR, Subject>::exercise_good(Subject::ZERO, Subject::CONSTANT, Subject::PRIO3_COUNT, Subject::PRIO3_SUM, Subject::PRIO3_HISTOGRAM, Subject::POPLAR1);
  }
  template<required::Indicator INDICATOR> auto Name::test_to_string_view_fail() -> void {
    To_String_View<INDICATOR, Subject>::exercise_fail(Subject{999'999'999u}, Subject{888'888'888});
  }
}
