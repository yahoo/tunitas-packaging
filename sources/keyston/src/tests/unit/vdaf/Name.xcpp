#divert <fpp>
#import tests.unit.name
namespace tests::unit::vdaf {
  namespace [[eponymous]] eponymous_name {
    using namespace tests::unit::name;
    namespace required = tests::unit::name::required; // else tests::unit::vdaf::required takes precedence
    class Name;
  }
  using eponymous_name::Name;
}
