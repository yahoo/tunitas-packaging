#divert <ipp>
#import std.get
namespace tunitas::keyston::vdaf {
  template<Size N, required::Definition DEFINITION> auto disassembled::get(Disassembled<DEFINITION> const &d) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type const & { return std::get<N>(d); } // the "tuple sense" will carry through
  template<Size N, required::Definition DEFINITION> auto disassembled::get(Disassembled<DEFINITION> &d) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type &             { return std::get<N>(d); }
  template<Size N, required::Definition DEFINITION> auto disassembled::get(Disassembled<DEFINITION> &&d) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type &&           { return std::get<N>(forward<Disassembled<DEFINITION>>(d)); }
}
#endiv
