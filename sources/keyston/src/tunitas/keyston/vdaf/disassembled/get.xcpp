#divert <hpp>
#import tunitas.keyston.vdaf.disassembled.Disassembled
#import tunitas.keyston.vdaf.required.Definition
namespace tunitas::keyston::vdaf::disassembled {
  //
  // get elements of the tuple.
  //
  // Specification:
  //
  //   Rhymes with std::get<...>(...)
  //   Findable by ADL.
  //
  // Design:
  //
  //
  // Usage:
  //
  //   You need a tutorial on this?
  //
  template<Size N, required::Definition DEFINITION> inline auto get(Disassembled<DEFINITION> const &) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type const &;
  template<Size N, required::Definition DEFINITION> inline auto get(Disassembled<DEFINITION> &) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type &;
  template<Size N, required::Definition DEFINITION> inline auto get(Disassembled<DEFINITION> &&) -> typename std::tuple_element<N, Disassembled<DEFINITION>>::type &&;
