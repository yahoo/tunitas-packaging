#divert <fpp>
#forward tunitas.denniston.service.client.Client
#forward tunitas.denniston.service.collector.Collector
#forward tunitas.denniston.service.gateway.Gateway
#forward tunitas.denniston.service.processor.Processor
namespace tests::unit::service::partial::required {
  //
  // Is this type supervisable?  Usable in the Supervisor<PROCESS>
  // Only certain processes (and not the subprocesses).
  //
  template<typename PROCESS> concept Supervisable = (same_as<PROCESS, library::client::Client> ||
                                                     same_as<PROCESS, library::collector::Collector> ||
                                                     same_as<PROCESS, library::gateway::Gateway> ||
                                                     same_as<PROCESS, library::processor::Processor>);
}
#endiv
