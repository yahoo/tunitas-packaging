#divert <fpp>
#import tunitas.denniston.scheduler.required.Invocable
#import tunitas.denniston.scheduler.required.World
namespace tests::unit::service::partial::required {
  //
  // Is this type an operate function (functoid, functor, funky?)
  //
  template<typename OPERATE, typename WORLD> concept Operate = requires(OPERATE operate, WORLD &world) {
    requires ::tunitas::denniston::scheduler::required::World<WORLD>;
    requires ::tunitas::denniston::scheduler::required::Invocable<OPERATE, WORLD &>;
    { operate(world) };
  };
}
#endiv
